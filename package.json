{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx serve -s web-build",
    "build": "npx expo export:web",
    "test": "jest --config jest.config.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@grafana/faro-react": "^1.10.2",
    "@grafana/faro-web-sdk": "^1.10.2",
    "@grafana/faro-web-tracing": "^1.10.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "expo": "~50.0.0",
    "firebase": "^10.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-gifted-chat": "^2.6.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "react-router-dom": "^6.27.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@grafana/faro-webpack-plugin": "^0.1.1",
    "@testing-library/react-native": "^12.7.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.0",
    "react-test-renderer": "18.2.0"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase|react-native-gifted-chat)"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
